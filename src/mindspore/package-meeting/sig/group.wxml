<!--package-meeting/sig/group.wxml-->
<view class="container">
    <view class="sig-list">
        <block wx:for="{{list}}" wx:key="index">
            <view class="item" bindtap="toAddMember" data-id="{{item.id}}" data-name="{{item.group_name}}">
                <text>{{item.group_name}}</text>
                <image src="./../../static/common/arrow-right.svg" />
            </view>
        </block>
    </view>
</view>
